{
  "name": "safe-front",
  "version": "0.1.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ci": "ng build --prod --sourceMap=false",
    "build:az:front": "ng build --configuration=azure --sourceMap=false --project=front-office",
    "build:az:back": "ng build --configuration=azure --sourceMap=false --deploy-url /backoffice/ --base-href /backoffice --project=back-office",
    "build:elem": "ng build web-element --aot --build-optimizer --extract-licenses --no-namedChunks --optimization --no-sourceMap --no-vendorChunk",
    "bundle:elem": "cat dist/web-element/runtime.js dist/web-element/polyfills.js dist/web-element/main.js > dist/web-element.js",
    "compodoc:front": "npx compodoc -p tsconfig.json projects/front-office/src -d documentation/front-office",
    "compodoc:back": "npx compodoc -p tsconfig.json projects/back-office/src -d documentation/back-office",
    "compodoc:safe": "npx compodoc -p tsconfig.json projects/safe/src -d documentation/safe",
    "test": "ng test",
    "test:ci": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^11.2.13",
    "@angular/animations": "^11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/elements": "^11.2.14",
    "@angular/forms": "^11.2.14",
    "@angular/localize": "^11.2.14",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/router": "^11.2.14",
    "@apollo/client": "^3.3.19",
    "@azure/msal-angular": "^1.1.2",
    "@progress/kendo-angular-buttons": "^5.0.0",
    "@progress/kendo-angular-charts": "^4.2.0",
    "@progress/kendo-angular-common": "^1.0.0",
    "@progress/kendo-angular-dateinputs": "^4.0.0",
    "@progress/kendo-angular-dialog": "^4.0.0",
    "@progress/kendo-angular-dropdowns": "^4.0.0",
    "@progress/kendo-angular-editor": "^1.2.4",
    "@progress/kendo-angular-excel-export": "^3.0.0",
    "@progress/kendo-angular-grid": "^4.8.0",
    "@progress/kendo-angular-inputs": "^6.0.0",
    "@progress/kendo-angular-intl": "^2.0.0",
    "@progress/kendo-angular-l10n": "^2.0.0",
    "@progress/kendo-angular-layout": "4.1.5 - 5",
    "@progress/kendo-angular-pdf-export": "^2.0.0",
    "@progress/kendo-angular-popup": "^3.0.0",
    "@progress/kendo-angular-scheduler": "^2.1.2",
    "@progress/kendo-angular-toolbar": "^3.0.1",
    "@progress/kendo-data-query": "^1.5.5",
    "@progress/kendo-date-math": "^1.3.2",
    "@progress/kendo-drawing": "^1.10.1",
    "@progress/kendo-licensing": "^1.1.4",
    "@progress/kendo-recurrence": "^1.0.0",
    "@progress/kendo-theme-default": "latest",
    "apollo-angular": "^2.4.0",
    "document-register-element": "^1.14.10",
    "graphql": "^15.0.0",
    "hammerjs": "^2.0.8",
    "leaflet": "^1.7.1",
    "leaflet.markercluster": "^1.4.1",
    "msal": "^1.4.11",
    "rxjs": "~6.6.7",
    "subscriptions-transport-ws": "^0.9.18",
    "survey-angular": "^1.8.48",
    "survey-creator": "^1.8.48",
    "survey-knockout": "^1.8.48",
    "surveyjs-widgets": "^1.8.48",
    "tslib": "^2.0.0",
    "uuid": "^8.3.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.13",
    "@angular/cli": "^11.2.13",
    "@angular/compiler-cli": "^11.2.14",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/leaflet": "^1.5.19",
    "@types/leaflet.markercluster": "^1.4.3",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.4",
    "codelyzer": "^6.0.0",
    "husky": "^4.3.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jira-prepare-commit-msg": "^1.6.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.1.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "jira-prepare-commit-msg",
      "pre-push": "ng lint"
    }
  },
  "jira-prepare-commit-msg": {
    "messagePattern": "[$J] $M",
    "jiraTicketPattern": "([A-Z]+-\\d+)",
    "commentChar": "#",
    "isConventionalCommit": false,
    "allowEmptyCommitMessage": false
  }
}
